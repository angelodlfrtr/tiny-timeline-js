(function(){"use strict";var Timeline=function(element,duration,opts){if(opts===undefined){opts={}}this.duration=duration;this.opts=opts;this.element=element;this.lines={};this.state=null;this.timestamp=0;this.createCursor()};Timeline.prototype.addLine=function(name,opts){var self=this;if(opts==undefined){opts={}}opts["events"]={};var l=this.lines[name]=opts;this.appendLine(l);l.addEvent=function(name,opts){l.events[name]=opts;self.appendEvent(l,name,opts)};return l};Timeline.getLine=function(name){return this.lines[name]};Timeline.prototype.appendEvent=function(line,event_name,event_opts){var event_element=document.createElement("div");event_element.classList.add("event");var total_width=this.element.clientWidth;var element_width=(event_opts["end"]-event_opts["start"])*100/this.duration*total_width/100;var element_margin_left=event_opts["start"]*100/this.duration*total_width/100;event_element.style["margin-left"]=element_margin_left+"px";event_element.style.width=element_width+"px";line.element.appendChild(event_element)};Timeline.prototype.appendLine=function(line){var line_element=document.createElement("div");line_element.classList.add("line");if(line["color"]){line_element.classList.add(line["color"])}this.element.appendChild(line_element);var cursor_element=this.getCursorElement();cursor_element.style.height=this.element.clientHeight+"px";line.element=line_element;return line_element};Timeline.prototype.createCursor=function(){var cursor_element=document.createElement("div");cursor_element.classList.add("cursor");this.element.appendChild(cursor_element)};Timeline.prototype.getCursorElement=function(){return this.element.querySelector(".cursor")};window.Timeline=Timeline}).call(this);var element=document.querySelector("#timeline");var test=new Timeline(element,6e4,{});var lineone=test.addLine("Gestes",{color:"red"});var lineone=test.addLine("Macho",{color:"red"});lineone.addEvent("Levage de bras",{start:1e3,end:3e3});lineone.addEvent("Levage de pieds",{start:3050,end:7e3});linetwo=test.addLine("Ta soeur",{color:"green"});linetwo.addEvent("Super",{start:2500,end:6e3});